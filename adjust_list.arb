module AdjustList
  def adjust_list
    buffer_answer = []
    self.each do |node_a, node_b|
      node_a -= 1
      node_b -= 1
      buffer_answer[node_a] ||= []
      buffer_answer[node_a] << node_b
      buffer_answer[node_b] ||= []
      buffer_answer[node_b] << node_a
    end
    buffer_answer
  end
end
